<h1><%=@employee.first_name%> <%=@employee.last_name%></h1>

<img src=<%= @employee.img_url%> height="215">
<ul>
  <li>Alias: <%= @employee.alias %></li>
  <li>Title: <%= @employee.title %></li>
  <li>Office: <%= @employee.office %></li>
